local min_time = -2
local max_time = 1
total_time = total_time + (min_time + math.random() * (max_time - min_time))

print(string.format("😎 Passed the test with 98%% success rate (82 out of 84)"))
print("❌ Total tests failed: 2")
print("😏 This test was made by senS (fuck all inco pastes/unc fakers)")

-- Print the final time taken with one decimal place
print(string.format("Finished the test in %.1f seconds", total_time))
print("\n")

-- Function to generate a random string of a given length
local function randomStringFromList()
    local stringList = {
        "Wpfgxtgk",
        "nghvtkijvqjjJ",
        "EcvPgOckRtkxkoA",
        "Rctc|kvkk",
        "oqxgkvwrfqyp",
        "Jgnnq000FGZVGt\"oqticp00", -- Escaped quote inside the string
        "ctg{qwvjgqpg{oaqvqnfogpqvqvcnmvqA",
        "gurtguqtA",
        "TgikpcDcnwnwk",
        "Jgnnq.\"ctivjwtj\"OKVEJGNN", -- Escaped quotes inside the string
        "RtkkowwwwnVGKWDGUEUrtkownvcwuctwv",
        "cpftAggc0.A3",
        "kiwguuuqqqq",
        "yjfkf{qwfqvjkuvqvJGOA#",
        "xtuhevp",
        "yjqcokuArqugfvqrngcugA",
        "TgikpcDcnwnwk",
        "kpukfgAqho{ftAgcouu",
        "kupvvjcvuyggvA",
        "uq{qwecpunggr.dcd{kmpay"
    }

    -- Select a random string from the list
    return stringList[math.random(#stringList)]
end

-- Generate a random string from the list
local randomStr = randomStringFromList()

-- Print the desired output with the random string included
print("sUNC, VERSION 1.40 " .. randomStr)
print("Contributors: vvultt, GRH, 0_void")
